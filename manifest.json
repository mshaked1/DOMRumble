{
  "manifest_version": 2,

  "name": "DOMSpin",
  "description": "This extension manipulates DOM elements in a 3D render",
  "version": "0.0.1",
  "content_scripts": [
        {
            "matches": ["http://*/*"],
            "js": ["js/three.js"]
        }
    ],
  "browser_action": {
    "default_icon": "/images/logo.gif",
    "default_title": "Click here to spin shit"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
